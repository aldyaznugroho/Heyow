<?xml version="1.0" encoding="utf-8"?>
<com.stepstone.stepper.StepperLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/add_match_stepper_layout"
    android:orientation="vertical"
    app:ms_stepperType="progress_bar"
    app:ms_backButtonText="Prev"
    app:ms_nextButtonText="Next"
    app:ms_backButtonColor="@color/color_white"
    app:ms_nextButtonColor="@color/color_white"
    app:ms_completeButtonColor="@color/color_white"
    app:ms_activeStepColor="@color/color_white"
    app:ms_bottomNavigationBackground="@color/colorPrimary"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.skripsigg.heyow.ui.addmatch.AddNewMatchActivity"
    tools:showIn="@layout/activity_add_new_match"/>
